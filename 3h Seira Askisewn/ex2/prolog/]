myAppend([],E,[E]).
myAppend([H|T],E,[H|T1]):-myAppend(T,E,T1).


countNumber([],_,C,C).
countNumber([H|T],N,C,A):-
    ( H == N -> C1 is C + 1
    ; C1 is C
    ),
    countNumber(T,N,C1,A).

returnKey(L,N,key(N,A)):-
    countNumber(L,N,0,A).
createKeyList([],_,_).
createKeyList([H|T],[Key|Rest],Checked):-
    ( \+ member(H,Checked) -> returnKey([H|T],H,Key),
    myAppend(Checked,H,Checked1),
    createKeyList(T,Rest,Checked1)
    ; createKeyList(T,Rest,Checked)
    ).

    
